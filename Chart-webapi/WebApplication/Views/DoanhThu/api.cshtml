                <tr>
                    <th>Chuyền</th>
                    <th>Mã hàng</th>
                    <th>Số sản phẩm</th>
                    <th>Đơn giá</th>
                    <th>Doanh thu</th>
                </tr>
            </thead>

            <tbody>
              
            </tbody>

        </table>
        <!--end: Datatable -->
    </div>
</div>

